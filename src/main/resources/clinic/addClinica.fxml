<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="155.0" prefWidth="379.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.view.Add.AddClinicaController">
   <center>
      <GridPane prefHeight="82.0" prefWidth="500.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox prefHeight="65.0" prefWidth="379.0">
               <children>
                  <Label text="Nome">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="nomeField">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="70.0" prefWidth="421.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="adicionarButton" defaultButton="true" layoutX="67.0" layoutY="23.0" mnemonicParsing="false" onAction="#adicionarHandler" text="Adicionar" />
            <Button fx:id="cancelarButton" cancelButton="true" layoutX="233.0" layoutY="23.0" mnemonicParsing="false" onAction="#cancelarHandler" text="Cancelar" />
         </children>
         <BorderPane.margin>
