<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" prefHeight="241.0" prefWidth="496.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.view.Add.AddUtenteController">
    <center>
        <GridPane fx:id="gridPane" prefHeight="82.0" prefWidth="500.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <VBox prefHeight="65.0" prefWidth="379.0">
                    <children>
                        <Label text="Nome *">
                            <VBox.margin>
                                <Insets left="5.0" right="5.0" top="5.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="nomeField">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                            </VBox.margin>
                        </TextField>
                    </children>
                </VBox>
            <VBox layoutX="10.0" layoutY="10.0" prefHeight="65.0" prefWidth="379.0" GridPane.columnIndex="1">
               <children>
                  <Label text="Telemovel">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="telField">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <VBox layoutX="261.0" layoutY="10.0" prefHeight="65.0" prefWidth="379.0" GridPane.rowIndex="1">
               <children>
                  <Label text="Morada">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="moradaField">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <VBox layoutX="10.0" layoutY="101.0" prefHeight="65.0" prefWidth="379.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label text="Clinica *">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </Label>
                  <ChoiceBox fx:id="clinicaChooser" prefHeight="25.0" prefWidth="183.0">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </ChoiceBox>
               </children>
            </VBox>
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </BorderPane.margin>
        </GridPane>
    </center>
    <bottom>
        <AnchorPane prefHeight="65.0" prefWidth="502.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="adicionarButton" defaultButton="true" layoutX="68.0" layoutY="20.0" mnemonicParsing="false" onAction="#adicionarHandler" text="Adicionar" />
                <Button fx:id="cancelarButton" cancelButton="true" layoutX="315.0" layoutY="20.0" mnemonicParsing="false" onAction="#cancelarHandler" text="Cancelar" />
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" />
            </BorderPane.margin>
        </AnchorPane>
    </bottom>
</BorderPane>
