<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="181.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clinic.view.Add.AddConsultaOutController">
    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <GridPane prefHeight="140.0" prefWidth="400.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                            <children>
                                <Label text="Data *">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0" top="5.0" />
                                    </VBox.margin>
                                </Label>
                                <DatePicker fx:id="dataField" prefHeight="25.0" prefWidth="293.0" showWeekNumbers="true">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                    </VBox.margin>
                                </DatePicker>
                        <Separator prefWidth="200.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </Separator>
                            </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
                            <children>
                                <Label text="Hora *">
                                    <VBox.margin>
                                        <Insets left="5.0" right="5.0" top="5.0" />
                                    </VBox.margin>
                                </Label>
                                <TextField fx:id="horaField">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                    </VBox.margin>
                                </TextField>
                        <Separator prefWidth="200.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                        </Separator>
                            </children>
                        </VBox>
                  <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
                     <children>
                        <Label text="Utente *">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                        </Label>
                        <ChoiceBox fx:id="choiceBox" prefHeight="25.0" prefWidth="300.0">
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                        </ChoiceBox>
                     </children>
                  </VBox>
                  <VBox layoutX="10.0" layoutY="79.0" prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <children>
                        <Label text="Pesquisar">
                           <VBox.margin>
                              <Insets left="5.0" right="5.0" top="5.0" />
                           </VBox.margin>
                        </Label>
                        <HBox>
                           <children>
                              <TextField fx:id="nomeField" promptText="Nome">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </TextField>
                              <Button fx:id="pesqButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="103.0" text="Pesquisar">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                    </children>
                </GridPane>
            </children>
        </VBox>
    </center>
    <bottom>
        <AnchorPane prefHeight="50.0" prefWidth="400.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="addButton" defaultButton="true" layoutX="72.0" layoutY="13.0" mnemonicParsing="false" text="Adicionar" />
                <Button fx:id="cancelButton" cancelButton="true" layoutX="275.0" layoutY="13.0" mnemonicParsing="false" text="Cancelar" />
            </children>
        </AnchorPane>
    </bottom>
</BorderPane>
